[encoding] {UTF-8}
  [exec] (xterm) {xterm}
  [exec] (Firefox) {firefox-quantum}
  [exec] (Chrome) {google-chrome-stable}
  [exec] (Evince - document reader) {evince}
  [exec] (Gimp - image editor) {gimp}
  [exec] (Thunar - window manager) {thunar}
  [exec]   (Run) {fbrun}
[submenu] (Settings)
  [exec] (Sound) {pavucontrol}
  [exec] (Graphics) {nvidia-settings}
  [config] (Configure)
  [workspaces] (Workspace List)
  [submenu] (Backgrounds)
    [wallpapers] (~/.fluxbox/backgrounds) {feh --bg-scale}
    [wallpapers] (~/Pictures/backgrounds) {feh --bg-scale}
  [end]
  [submenu] (Keyboard)
    [exec] (normal) {setxkbmap us}
    [exec] (international) {setxkbmap us -variant intl}
  [end]
  [submenu] (Tools)
    [exec] (Run) {fbrun}
    [exec] (Regen Menu) {fluxbox-generate_menu}
  [end]
    [commanddialog] (Fluxbox Command)
    [reconfig] (Reload config)
    [restart] (Restart)
    [separator]
    [exit] (Exit)
  [end]
[endencoding]
